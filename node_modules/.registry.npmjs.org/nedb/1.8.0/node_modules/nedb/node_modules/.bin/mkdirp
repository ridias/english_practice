#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/cmd.js/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/node_modules:/mnt/c/Users/user/Documents/Programacion/node_modules:/mnt/c/Users/user/Documents/node_modules:/mnt/c/Users/user/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../mkdirp/0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/cmd.js/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/mkdirp/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/english_practice/node_modules:/mnt/c/Users/user/Documents/Programacion/Github/node_modules:/mnt/c/Users/user/Documents/Programacion/node_modules:/mnt/c/Users/user/Documents/node_modules:/mnt/c/Users/user/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../mkdirp/0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
  ret=$?
fi
exit $ret
